-- Accent é¢„è®¾
local AccentPresets = {
    ["è“è‰²"] = dp(88, 101, 242),
    ["ç´«è‰²"] = dp(155, 105, 245),
    ["ç»¿è‰²"] = dp(34, 197, 94),
