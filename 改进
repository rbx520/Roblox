fpsLabel.Parent.Parent.LayoutOrder = 9
createInfoLabel(InfoContainer, "æœåŠ¡å™¨ ID", game.JobId:sub(1, 18) .. "...", false).Parent.Parent.LayoutOrder = 10
createInfoLabel(InfoContainer, "åœ°å›¾ ID", game.PlaceId, false).Parent.Parent.LayoutOrder = 11

-- Ping å’Œ FPS çš„å®æ—¶æ›´æ–°
task.spawn(function()
    local lastUpdate = 0
    b.RunService.RenderStepped:Connect(function(dt)
        if tick() - lastUpdate > 0.5 then
            lastUpdate = tick()
            local pingItem = b.Stats.Network.ServerStatsItem["Data Ping"]
            local ping = pingItem and pingItem:GetValue() or 0
            pingLabel.Text = math.floor(ping) .. " ms"
            fpsLabel.Text = tostring(math.floor(1 / dt))
        end
    end)
end)

