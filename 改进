-- ÈªòËÆ§‰ΩøÁî®ÈªëËâ≤‰∏ªÈ¢ò
local CurrentTheme = "Dark"
local Theme = Themes[CurrentTheme]

getgenv().library = {
    flags = {
        GetState = function(dt, du)
            return library.flags[du] and library.flags[du].State or nil
        end
    },
    modules = {},
    currentTab = nil,
    isHomepageVisible = false, -- ÂÖºÂÆπÊóßÂ≠óÊÆµÔºàÂÆûÈôÖÁî± activeOverlay ÁÆ°Ôºâ
    theme = CurrentTheme,
    activeOverlay = nil, -- nil / "Profile" / "Settings"
    CustomTitle = nil
}

-- UTF8 ÂÆâÂÖ®Êà™Êñ≠Âà∞ÊúÄÂ§ö n ‰∏™Â≠óÁ¨¶Ôºà‰∏≠ÊñáÁÆó 1 ‰∏™Ôºâ
local function utf8SubMax(s, n)
    s = tostring(s or "")
    local ok, len = pcall(utf8.len, s)
    if ok and len then
        if len <= n then return s end
        local offset = utf8.offset(s, n + 1)
        if offset then
            return string.sub(s, 1, offset - 1)
        end
        return s
    else
        -- ÈÅáÂà∞ÈùûÊ≥ïÂ∫èÂàóÊó∂ÔºåÊâãÂä®ÊåâUTF8Ê≠•Ëøõ
        local count, i = 0, 1
        while i <= #s and count < n do
            local c = s:byte(i)
            local step = (c < 128 and 1) or (c < 224 and 2) or (c < 240 and 3) or (c < 248 and 4) or 1
            i = i + step
            count = count + 1
        end
        return s:sub(1, i-1)
    end
end

-- ÂàáÊç¢‰∏ªÈ¢ò
function library:SetTheme(themeName)
    if not Themes[themeName] then return end
    
    CurrentTheme = themeName
    Theme = Themes[themeName]
    library.theme = themeName
    
    if library.updateTheme then
        library.updateTheme()
    end
end

function library:UpdateToggle(du, be)
    local be = be or library.flags:GetState(du)
    if be == library.flags:GetState(du) then
        return
    end
    library.flags[du]:SetState(be)
end

local dv = {}

-- ‰ºòÂåñÁöÑTweenÂáΩÊï∞
function dv:Tween(props, object, duration, style, direction)
    local tween = b.TweenService:Create(
        object,
        TweenInfo.new(
            duration or 0.2,
            style or Enum.EasingStyle.Quad,
            direction or Enum.EasingDirection.Out
        ),
        props
    )
    tween:Play()
    return tween
end

-- Áªü‰∏ÄË¶ÜÁõñÂ±ÇÈöêËóèÔºà‰øÆÂ§çÁâàÔºâ
function dv:HideOverlay()
    local gui = b.CoreGui[_CONFIGS.UI_NAME].Main
    local ProfilePage = gui.ProfilePage
    local SettingsPage = gui.SettingsPage

    if ProfilePage then ProfilePage.Visible = false end
    if SettingsPage then SettingsPage.Visible = false end

    library.activeOverlay = nil
    library.isHomepageVisible = false

    -- ÂèñÊ∂àÊåâÈíÆÈ´ò‰∫Æ
    if gui.Header:FindFirstChild("ProfileButton") then
        dv:Tween({BorderColor3 = Theme.Secondary}, gui.Header.ProfileButton.ProfileStroke, 0.15)
    end
    if gui.Header:FindFirstChild("SettingsButton") then
        dv:Tween({BorderColor3 = Theme.Secondary}, gui.Header.SettingsButton.SettingsStroke, 0.15)
    end

    -- ÊÅ¢Â§çÂΩìÂâçÊ†áÁ≠æÈ°µÔºàÊó†ËÆ∫activeOverlayÊòØÂê¶‰∏∫nilÈÉΩË¶ÅÊÅ¢Â§çÔºâ
    if library.currentTab then
        library.currentTab[1].Visible = true
        if library.currentTab[2] and library.currentTab[2]:FindFirstChild("Glow") then
            dv:Tween({BackgroundTransparency = 0}, library.currentTab[2].Glow, 0.15)
        end
    end
end

-- ÊòæÁ§∫Êüê‰∏™Ë¶ÜÁõñÂ±ÇÔºàProfile/SettingsÔºâ
function dv:ShowOverlay(which)
    local gui = b.CoreGui[_CONFIGS.UI_NAME].Main
    local ProfilePage = gui.ProfilePage
    local SettingsPage = gui.SettingsPage
    local ProfileStroke = gui.Header.ProfileButton.ProfileStroke
    local SettingsStroke = gui.Header.SettingsButton.SettingsStroke

    -- Â∑≤ÁªèÂú®ËØ•È°µÂàôÂÖ≥Èó≠
    if library.activeOverlay == which then
        dv:HideOverlay()
        return
    end

    -- ÂàáÊç¢ÂâçÂÖàÈöêËóè
    dv:HideOverlay()

    -- ÈöêËóèÊ†áÁ≠æÈ°µÂÜÖÂÆπ
    if library.currentTab then
        library.currentTab[1].Visible = false
        if library.currentTab[2] and library.currentTab[2]:FindFirstChild("Glow") then
            dv:Tween({BackgroundTransparency = 1}, library.currentTab[2].Glow, 0.15)
        end
    end

    if which == "Profile" then
        if ProfilePage then ProfilePage.Visible = true end
        library.activeOverlay = "Profile"
        library.isHomepageVisible = true
        dv:Tween({BorderColor3 = Theme.Accent}, ProfileStroke, 0.15)
        dv:Tween({BorderColor3 = Theme.Secondary}, SettingsStroke, 0.15)
    elseif which == "Settings" then
        if SettingsPage then SettingsPage.Visible = true end
        library.activeOverlay = "Settings"
        library.isHomepageVisible = false
        dv:Tween({BorderColor3 = Theme.Secondary}, ProfileStroke, 0.15)
        dv:Tween({BorderColor3 = Theme.Accent}, SettingsStroke, 0.15)
    end
end

-- ÂàáÊç¢Ê†áÁ≠æÔºà‰øÆÂ§çÁâàÔºâ
function dv:SwitchTab(newTab)
    -- ÂàáÊç¢Ê†áÁ≠æÊó∂ÔºåÁ°Æ‰øùË¶ÜÁõñÂ±ÇÈöêËóè
    dv:HideOverlay()
    
    local oldTab = library.currentTab
    
    -- Âç≥‰ΩøÊòØÂêå‰∏Ä‰∏™Ê†áÁ≠æ‰πüË¶ÅÁ°Æ‰øùÊòæÁ§∫Ôºà‰øÆÂ§çÁ©∫ÁôΩÈ°µÈù¢ÈóÆÈ¢òÔºâ
    if oldTab == newTab then 
        newTab[1].Visible = true
        if newTab[2] and newTab[2]:FindFirstChild("Glow") then
            dv:Tween({BackgroundTransparency = 0}, newTab[2].Glow, 0.15)
        end
        return 
    end
    
    library.currentTab = newTab
    
    if oldTab then
        if oldTab[2] and oldTab[2]:FindFirstChild("Glow") then
            dv:Tween({BackgroundTransparency = 1}, oldTab[2].Glow, 0.15)
        end
        oldTab[1].Visible = false
    end
    
    if newTab[2] and newTab[2]:FindFirstChild("Glow") then
        dv:Tween({BackgroundTransparency = 0}, newTab[2].Glow, 0.15)
    end
    newTab[1].Visible = true
end

