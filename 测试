-- ä¼ é€åˆ°ç©å®¶
gameTab:NewButton("ä¼ é€åˆ°ç›®æ ‡", function()
    if getgenv().ScriptSettings.TargetPlayer ~= "" then
        local targetPlayer = Players:FindFirstChild(getgenv().ScriptSettings.TargetPlayer)
        if targetPlayer and targetPlayer.Character and targetPlayer.Character:FindFirstChild("HumanoidRootPart") then
            LocalPlayer.Character.HumanoidRootPart.CFrame = targetPlayer.Character.HumanoidRootPart.CFrame
            library:Notify({
                Title = "ä¼ é€æˆåŠŸ",
                Text = "å·²ä¼ é€åˆ° " .. getgenv().ScriptSettings.TargetPlayer,
                Duration = 3
            })
        else
            library:Notify({
                Title = "ä¼ é€å¤±è´¥",
                Text = "æ‰¾ä¸åˆ°ç›®æ ‡ç©å®¶",
                Duration = 3
            })
        end
    else
        library:Notify({
            Title = "é”™è¯¯",
            Text = "è¯·å…ˆè®¾ç½®ç›®æ ‡ç©å®¶",
            Duration = 3
        })
    end
end)

-- è§‚å¯Ÿç©å®¶
gameTab:NewButton("è§‚å¯Ÿç›®æ ‡", function()
    if getgenv().ScriptSettings.TargetPlayer ~= "" then
        local targetPlayer = Players:FindFirstChild(getgenv().ScriptSettings.TargetPlayer)
        if targetPlayer then
            workspace.CurrentCamera.CameraSubject = targetPlayer.Character.Humanoid
            library:Notify({
                Title = "è§‚å¯Ÿæ¨¡å¼",
                Text = "æ­£åœ¨è§‚å¯Ÿ " .. getgenv().ScriptSettings.TargetPlayer,
                Duration = 3
            })
        end
    end
end)

