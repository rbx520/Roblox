	Page2:NewButton("é”å®šè§†è§’è„šæœ¬", function()
		xpcall(function()
			if HONG.LP.PlayerGui:FindFirstChild("Shiftlock (StarterGui)") then
				return
			end
			local a = Instance.new("ScreenGui")
