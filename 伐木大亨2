	function HONG:NOTIFY(title, text, duration) --> é€šçŸ¥åŠŸèƒ½
	    return library:Notify({
	           Title = title, 
	           Text = text,
	           Duration = duration
	       })
	end
	
	--â†“ åŒå¼•å·é‡Œé¢çš„ä¸­æ–‡å¯ä»¥ä¿®æ”¹, å¯¹åº”çš„æ˜¯è„šæœ¬UIçš„èœå•åå­—
	local Page1 = library:CreateTab("è®¾ç½®èœå•");
	local Page2 = library:CreateTab("ç©å®¶èœå•");
	local Page3 = library:CreateTab("è´­ä¹°èœå•");
	local Page4 = library:CreateTab("æœ¨æèœå•");
	local Page5 = library:CreateTab("ä¼ é€èœå•");
	local Page6 = library:CreateTab("ç¯å¢ƒèœå•");
	local Page7 = library:CreateTab("åŸºåœ°èœå•");
	local Page8 = library:CreateTab("æ±½è½¦èœå•");
	
	
	Page1:NewToggle("é˜²è¯¯è§¦", "Mistouch", true, function(v)
	    _CONFIGS["é˜²è¯¯è§¦å¼€å…³"] = v;
	end)
	
	Page1:NewButton("å…³é—­è„šæœ¬", function()
	    	if _CONFIGS["é˜²è¯¯è§¦å¼€å…³"] == true then
			HONG:SelectNotify("é˜²è¯¯è§¦", "ç¡®å®šè¦å…³é—­è„šæœ¬å—?", "ç¡®å®š", "å–æ¶ˆ", 5, function(text) --> åŒå¼•å·é‡Œé¢çš„ä¸­æ–‡å¯ä»¥æ”¹
				if text == "ç¡®å®š" then
					xpcall(function()
						for i, v in next, HONG.COREGUI:GetDescendants() do
							if v.Name == _CONFIGS.UI_NAME then
								v:Destroy()
								ClearConfig()
							end
						end
					end, function(err)
						return HONG:printf("é”™è¯¯æ˜¯:  %s", err)
					end)
					return
				end
				HONG:NOTIFY("é€šçŸ¥", "å·²å–æ¶ˆ", 4) --> åŒå¼•å·é‡Œé¢çš„ä¸­æ–‡å¯ä»¥æ”¹
			end)
			return
		end
		xpcall(function()
			for i, v in next, HONG.COREGUI:GetDescendants() do
				if v.Name == _CONFIGS.UI_NAME then
					v:Destroy()
					ClearConfig()
				end
			end
		end, function(err)
			return HONG:printf("é”™è¯¯æ˜¯:  %s", err)
		end)
	end)
	
	Page1:NewButton("é‡è¿›æœåŠ¡å™¨", function()
		if _CONFIGS["é˜²è¯¯è§¦å¼€å…³"] == true then
			HONG:SelectNotify("é˜²è¯¯è§¦", "ç¡®å®šè¦é‡è¿›æœåŠ¡å™¨å—?", "ç¡®å®š", "å–æ¶ˆ", 5, function(text) --> åŒå¼•å·é‡Œé¢çš„ä¸­æ–‡å¯ä»¥æ”¹
				if text == "ç¡®å®š" then
					xpcall(function()
						HONG:ServiceTP(game.PlaceId)
					end, function(err)
						return HONG:printf("é”™è¯¯æ˜¯:  %s", err)
					end)
					return
				end
				HONG:NOTIFY("é€šçŸ¥", "å·²å–æ¶ˆ", 4) --> åŒå¼•å·é‡Œé¢çš„ä¸­æ–‡å¯ä»¥æ”¹
			end)
			return
		end
		xpcall(function()
			HONG:ServiceTP(game.PlaceId)
		end, function(err)
			return HONG:printf("é”™è¯¯æ˜¯:  %s", err)
		end)
	end)
	
	Page1:NewSeparator()
	
	
	Page1:NewButton("è„šæœ¬ä½œè€…  ç´…#0296", function()
		print("æ„Ÿè°¢ ç´…")
	end)
	
	
	Page1:NewButton("Woof UI Library  0x37", function()
		print("æ„Ÿè°¢ Step")
	end)
	
	Page1:NewButton("æ— çœ¼çƒç å¹»å½±  cooper#2723", function()
		print("æ„Ÿè°¢ cooper")
	end)
	
