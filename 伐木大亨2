	                bp.Parent = HONG.WKSPC.Region_Volcano
	                bp:ClearAllChildren()
	                bp.CanCollide = true
	                bp.BrickColor = HONG.WKSPC.Region_Volcano.Slate.BrickColor
	                bp.Material = HONG.WKSPC.Region_Volcano.Slate.Material
	                v.Parent = HONG.LIGHT
	            end
	        end
	    end
	end)
	
