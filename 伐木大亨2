createInfoLabel(InfoContainer, "æ˜¾ç¤ºåç§°", player.DisplayName, false).Parent.Parent.LayoutOrder = 2
createInfoLabel(InfoContainer, "ç”¨æˆ· ID", player.UserId, false).Parent.Parent.LayoutOrder = 3
createInfoLabel(InfoContainer, "è´¦å·å¹´é¾„", player.AccountAge, false).Parent.Parent.LayoutOrder = 4

